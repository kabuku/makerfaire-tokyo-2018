{
  "compilerOptions": {
    "esModuleInterop": true,
    "target": "es5",
    "importHelpers": true,
    "module": "es2015",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "lib": ["es2015", "DOM"],
    "strict": true,
    "skipLibCheck": true,
    "suppressImplicitAnyIndexErrors": true
  },
  "exclude": ["node_modules"]
}
