{
  "name": "makerfaire-tokyo-2018",
  "version": "0.1.0",
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.7.0"
  },
  "scripts": {
    "load-model": "node scripts/loadMobilenet",
    "web": "parcel --https web/index.html web/pairplay.html",
    "mqtt": "npm run copy-key && docker run -it -p 1883:1883 -p 9001:9001 -v $(pwd)/mqtt/mosquitto:/mosquitto eclipse-mosquitto:1.4.12",
    "copy-key": "cp .cache/primary.crt mqtt/mosquitto/cert/primary.crt && cp .cache/private.pem mqtt/mosquitto/cert/private.pem",
    "precommit": "lint-staged",
    "ci": "tsc && tslint \"web/**/*.ts\" -p tsconfig.json"
  },
  "license": "UNLICENSED",
  "dependencies": {
    "@tensorflow/tfjs": "^0.11.6",
    "mqtt": "^2.18.1",
    "node-fetch": "^2.1.2",
    "rxjs": "^6.2.1",
    "tslib": "^1.9.2"
  },
  "devDependencies": {
    "@types/mqtt": "^2.5.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "parcel-bundler": "^1.9.1",
    "prettier": "^1.13.5",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-config-standard": "^7.0.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.9.2"
  },
  "lint-staged": {
    "web/**/*.ts": [
      "tslint --fix -p tsconfig.json",
      "git add"
    ]
  }
}
